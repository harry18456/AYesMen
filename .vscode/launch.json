{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run AYesMan Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}/ayesman"
			],
			"outFiles": [
				"${workspaceFolder}/ayesman/out/**/*.js"
			],
			"preLaunchTask": "watch ayesman"
		}
	]
}
