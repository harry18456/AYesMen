{
  "name": "ayesman",
  "displayName": "AYesMan",
  "description": "Auto-accept agent steps and display model quota for Antigravity",
  "version": "0.1.0",
  "publisher": "ayesmen",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ayesman.refreshQuota",
        "title": "AYesMan: Refresh Quota"
      },
      {
        "command": "ayesman.toggleAutoAccept",
        "title": "AYesMan: Toggle Auto-Accept"
      },
      {
        "command": "ayesman.diagnose",
        "title": "AYesMan: Diagnose Auto-Accept"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "esbuild src/extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/node": "20.x",
    "typescript": "^5.4.5",
    "esbuild": "^0.21.5"
  }
}
